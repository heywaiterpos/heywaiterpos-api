{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": [
      ".env",
      "assets/*",
      "**/*.hbs",
      "public/css/*",
      "public/js/*"],
    "plugins": ["@nestjs/swagger"]
  }
}
